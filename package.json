{
  "name": "decorator-utils",
  "version": "0.0.1",
  "description": "Tiny helper for js decorator building",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --require babel-core/register --reporter spec test --recursive",
    "lint": "standard-flow src/**/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.qiwi.com:jslab/decorator-utils.git"
  },
  "keywords": [
    "decorator",
    "@decorator"
  ],
  "author": "Anton Golub <a.golub@qiwi.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "flow-bin": "^0.66.0",
    "flow-coverage-report": "^0.4.1",
    "flow-remove-types": "^1.2.3",
    "glob": "^7.1.2",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "standard-flow": "^1.0.0"
  },
  "standard-flow": {
    "parser": "babel-eslint",
    "globals": [
      "it",
      "expect",
      "describe"
    ],
    "ignore": [],
    "plugins": [
      "flowtype"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  },
  "dependencies": {}
}
